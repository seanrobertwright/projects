<mvc:View
	controllerName="sap.m.sample.QuickViewFallbackIcon.QuickViewFallbackIcon"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout"
	height="100%">
	<Page
		showHeader="false"
		class="sapUiContentPadding"
		showNavButton="false">
		<l:VerticalLayout>
			<Button
				id="showQuickView"
				class="sapUiSmallMarginBottom"
				text="Employee QuickView"
				width="200px"
				press=".handleButtonPress" />
		</l:VerticalLayout>
	</Page>
</mvc:View>