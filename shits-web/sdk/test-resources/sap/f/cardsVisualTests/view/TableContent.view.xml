<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.f"
	xmlns:w="sap.ui.integration.widgets"
	displayBlock="true">

	<Page title="{i18n>appTitle}">

		<subHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="Table Content Card "/>
				<ToolbarSpacer/>
			</Toolbar>
		</subHeader>

		<content>

			<Panel>
				<w:Card id="tablecard1" manifest="{manifests>/tableManifests/tablecard1}" width="700px" height="auto"/>
				<w:Card id="tablecard2" manifest="{manifests>/tableManifests/tablecard2}" width="700px" height="auto" class="sapUiSmallMarginBegin"/>
			</Panel>
		</content>
	</Page>
</mvc:View>