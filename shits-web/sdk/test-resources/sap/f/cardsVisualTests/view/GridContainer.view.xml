<mvc:View
		xmlns="sap.m"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:f="sap.f"
		xmlns:w="sap.ui.integration.widgets"
		displayBlock="true">
	<Page title="{i18n>appTitle}">
		<subHeader>
			<Toolbar>
				<ToolbarSpacer />
				<Title text="Card Layout"/>
				<ToolbarSpacer />
			</Toolbar>
		</subHeader>
		<content>
			<f:GridContainer id="cContainer">
				<w:Card manifest="{manifests>/listContent/card13}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="3" />
					</w:layoutData>
				</w:Card>
				<w:Card manifest="{manifests>/noLayout/card7}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="4" />
					</w:layoutData>
				</w:Card>
				<w:Card manifest="{manifests>/noLayout/card7}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="4" />
					</w:layoutData>
				</w:Card>
				<w:Card manifest="{manifests>/noLayout/card4}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="3" />
					</w:layoutData>
				</w:Card>
				<w:Card manifest="{manifests>/listContent/card13}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="3" />
					</w:layoutData>
				</w:Card>
				<w:Card manifest="{manifests>/noLayout/card4}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="5" />
					</w:layoutData>
				</w:Card>
				<w:Card manifest="{manifests>/noLayout/card2}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="3" />
					</w:layoutData>
				</w:Card>

				<w:Card manifest="{manifests>/listContent/card13}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="5" />
					</w:layoutData>
				</w:Card>

				<w:Card manifest="{manifests>/noLayout/card6}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="8" />
					</w:layoutData>
				</w:Card>
			</f:GridContainer>
		</content>
	</Page>
</mvc:View>
