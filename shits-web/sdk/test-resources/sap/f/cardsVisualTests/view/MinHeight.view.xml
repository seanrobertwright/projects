<mvc:View
		xmlns="sap.m"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:f="sap.f"
		xmlns:w="sap.ui.integration.widgets"
		displayBlock="true">
	<Page title="{i18n>appTitle}">
		<subHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="Min height of the card content "/>
				<ToolbarSpacer/>
			</Toolbar>
		</subHeader>
		<content>
			<Panel id="panelInCozy" headerText="Cozy mode">
				<w:Card class="sapUiMediumMargin" manifest="{manifests>/minHeight/line}" width="400px"/>
				<w:Card class="sapUiMediumMargin" manifest="{manifests>/minHeight/stackedColumn}" width="400px"/>
				<w:Card class="sapUiMediumMargin" manifest="{manifests>/minHeight/stackedBar}" width="400px"/>
				<w:Card class="sapUiMediumMargin" manifest="{manifests>/minHeight/donut}" width="400px"/>
				<w:Card class="sapUiMediumMargin" manifest="{manifests>/minHeight/smallList}" width="400px"/>
				<w:Card class="sapUiMediumMargin" manifest="{manifests>/minHeight/mediumList}" width="400px"/>
				<w:Card class="sapUiMediumMargin" manifest="{manifests>/minHeight/largeList}" width="400px"/>
				<w:Card class="sapUiMediumMargin" manifest="./cardsVisualTests/cardcontent/objectcontent/staticData.json" width="400px"/>
				<w:Card class="sapUiMediumMargin" manifest="{manifests>/minHeight/table}" width="400px"/>
				<w:Card class="sapUiMediumMargin" manifest="{manifests>/minHeight/timeline}" width="400px"/>
			</Panel>
			<Panel id="panelInCompact" class="sapUiSizeCompact" headerText="Compact mode">
				<w:Card class="sapUiMediumMargin" manifest="{manifests>/minHeight/line}" width="400px"/>
				<w:Card class="sapUiMediumMargin" manifest="{manifests>/minHeight/stackedColumn}" width="400px"/>
				<w:Card class="sapUiMediumMargin" manifest="{manifests>/minHeight/stackedBar}" width="400px"/>
				<w:Card class="sapUiMediumMargin" manifest="{manifests>/minHeight/donut}" width="400px"/>
				<w:Card class="sapUiMediumMargin" manifest="{manifests>/minHeight/smallList}" width="400px"/>
				<w:Card class="sapUiMediumMargin" manifest="{manifests>/minHeight/mediumList}" width="400px"/>
				<w:Card class="sapUiMediumMargin" manifest="{manifests>/minHeight/largeList}" width="400px"/>
				<w:Card class="sapUiMediumMargin" manifest="./cardsVisualTests/cardcontent/objectcontent/staticData.json" width="400px"/>
				<w:Card class="sapUiMediumMargin" manifest="{manifests>/minHeight/table}" width="400px"/>
				<w:Card class="sapUiMediumMargin" manifest="{manifests>/minHeight/timeline}" width="400px"/>
			</Panel>
		</content>
	</Page>
</mvc:View>
