{
  "sap.app": {
	"type": "card"
  },
  "sap.card": {
	"type": "List",
	"header": {
	  "data": {
		"request": {
		  "url": "./cardcontent/delayedLoading/listData2.json"
		},
		"path": "/header"
	  },
	  "title": "{title}",
	  "subTitle": "Subtitle",
	  "status": {
		"text": "2 of {count}"
	  },
	  "icon": {
		"src": "{src}"
	  }

	},
	"content": {
	  "data": {
		"request": {
		  "url": "./cardcontent/delayedLoading/listData2.json"
		},
		"path": "/content"
	  },
	  "item": {
		"title": {
		  "label": "{{title_label}}",
		  "value": "{Name}"
		},
		"icon": {
		  "src": "{icon}"
		}
	  }
	}
  }
}
