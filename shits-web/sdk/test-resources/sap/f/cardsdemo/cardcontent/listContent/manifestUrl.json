{
  "sap.app": {
    "type": "card",
    "i18n": "i18n/i18n.properties"
  },
  "sap.card": {
    "configuration": {
      "parameters": {
        "city": {
          "value": "Vratza"
        },
        "country": {
          "value": "Bulgaria"
        }
      }
    },
    "type": "List",
    "header": {
      "title": "Default manifest parameters",
      "subTitle": "Default parameter from manifest"
    },
    "content": {
      "data": {
        "request": {
          "url": "./cardcontent/someitems.json"
        }
      },
      "item": {
        "icon": {
          "src": "{icon}"
        },
        "title": {
          "label": "{{title_label}}",
          "value": "{Name}, {{parameters.TODAY_ISO}}"
        },
        "description": {
          "value": "Stationed in: {{parameters.city}}, {{parameters.country}}"
        },
        "highlight": "{state}"
      }
    }
  }
}