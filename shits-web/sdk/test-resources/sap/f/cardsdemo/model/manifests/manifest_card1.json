{
	"sap.app": {
		"type": "card"
	},
	"sap.card": {
		"type": "List",
		"header": {
			"title": "Request list content Card",
			"subTitle": "Card Subtitle",
			"icon":  {
				"src": "sap-icon://accept"
			},
			"status": "100 of 200"
		},
		"content": {
			"data": {
				"request": {
					"url": "./cardcontent/someitems.json"
				}
			},
			"item": {
				"icon": {
					"src": "{icon}"
				},
				"title": {
					"value": "{Name}"
				},
				"description": {
					"value": "{Description}"
				}
			}
		}
	}
}