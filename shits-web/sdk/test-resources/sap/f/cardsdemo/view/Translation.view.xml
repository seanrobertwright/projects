<mvc:View
		xmlns="sap.m"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:w="sap.ui.integration.widgets"
		displayBlock="true">

	<Page title="{i18n>appTitle}">

		<subHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="Translations and Count"/>
				<ToolbarSpacer/>
			</Toolbar>
		</subHeader>

		<content>
			<Panel>
				<VBox>
					<HBox>
						<w:Card class="sapUiMediumMargin" manifest="./cardcontent/translation/manifest.json" width="400px" height="auto"/>
					</HBox>
				</VBox>
			</Panel>
		</content>
	</Page>
</mvc:View>