<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.f"
	xmlns:w="sap.ui.integration.widgets"
	displayBlock="true">

    <Page title="{i18n>appTitle}">

        <subHeader>
            <Toolbar>
                <ToolbarSpacer/>
                <Title text="No header / No content cards"/>
                <ToolbarSpacer/>
            </Toolbar>
        </subHeader>

        <content>
            <Panel>
                <w:Card id="i1" manifest="{manifests>/noHeaderNoContent/card131}" width="400px" height="auto" class="sapUiSmallMargin"/>
                <w:Card id="i2" manifest="{manifests>/noHeaderNoContent/card141}" width="400px" height="auto" class="sapUiSmallMargin"/>
            </Panel>
        </content>
    </Page>
</mvc:View>